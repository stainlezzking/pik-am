<script>
  


  // my codes
  const month = ["January","February","March","April","May","June","July","August","September","October","November","December"];
  let single = "";
  let sideContent = ""
  const imgDefault = "http://0.news.cdn.pik-service.ru/attachment/46000/46747/1_artboard-3_46a6cce4a660344cfe32e71318791d71_6779105e26920fa7e4fd726e26442223.jpg"
  const getBlogs = function(e){
    e.forEach((blog, index)=>{
    if(index == 0){
    let date = new Date(blog.publishedAt*1000)
    single = `
      <div class="appartment_img">
      <img src="${blog.preview || blog.previewWater || imgDefault}" alt="${blog.title}">
      </div>
      <div class="single_appartment_content">
      <p>${date.getDay() + " " + month[date.getMonth()] + " "+ date.getFullYear()}</p>
      <a target="_blank" href="https://pik-group.com/about/news-and-reports/news/${blog.id}">
      <h4>${blog.title}</h4>
      </a>
      </div>
      </div>
    `
    }else{
      let date = new Date(blog.publishedAt*1000)
      sideContent +=  `

      <div class="single_blog">
        <div class="media gap-2 d-flex">
        <img src="${blog.preview || blog.previewWater}" class="mr-3"  alt="${blog.title}">
        <div class="media-body align-self-center">
        <a target="_blank" href="https://pik-group.com/about/news-and-reports/news/${blog.id}">
        <h5 class="mt-0">${blog.title}</h5>
        </a>
        <ul class="list-unstyled">
        <li><span class="ti-time"></span> ${month[date.getMonth()] + " " +date.getDay()}</li>
        </ul>
        </div>
        </div>
    </div>
      `
    }
    })
      return {single, sideContent}
  }
  // fetching blogs
  fetch("https://api.pik.pikweb.net/v2/news/articles?orTags=mainpage-news-pik-group.com&limit=4&page=1")
    .then(res => res.json())
    .then(d=> d.items)
    .then(getBlogs)
    .then(template=>{
      document.getElementById("blog_loader").style.display = "none"
      document.getElementById("single_blog").innerHTML = template.single
      document.getElementById("right_blogs").innerHTML = template.sideContent
      
    })
</script>
