<!DOCTYPE html>
<html lang="en">

<head>

    <title>Dashboard | PIK ASSETS MANAGEMENT </title>

    <!-- headLinks -->
    <%- include("../partials/dashboard/headLinks.ejs") -%>
        <!-- ######### -->


        <style>
            .btn:not(.btn-shadow):not(.shadow):not(.shadow-sm):not(.shadow-lg):not(.shadow-xs) {
                box-shadow: none;
            }
            
            .btn.btn-outline.btn-outline-dashed {
                border-width: 1px;
                border-style: dashed;
            }
            
            .bg-disabled {
                background-color: #e1e0ff !important;
            }
        </style>
</head>

<body x-data class="is-header-blur" x-bind="$store.global.documentBody">
    <!-- App preloader-->
    <div class="app-preloader fixed z-50 grid h-full w-full place-content-center bg-slate-50 dark:bg-navy-900">
        <div class="app-preloader-inner relative inline-block h-48 w-48"></div>
    </div>

    <!-- importing copy to clipboard function -->
    <%- include("../partials/dashboard/components/clipboard.ejs") -%>
        <!-- #### -->

        <!-- Page Wrapper -->
        <div id="root" class="min-h-100vh flex grow bg-slate-50 dark:bg-navy-900" x-cloak>
            <!-- Sidebar -->
            <div class="sidebar print:hidden">

                <!-- main sidebar and sidebar panel -->
                <%- include("../partials/dashboard/components/nav/navbar.ejs") -%>
                    <!-- ### -->
            </div>

            <!-- Here lies the  App heder -->
            <%- include("../partials/dashboard/components/nav/header.ejs") -%>
                <!-- ######  App header:end ###-->

                <!-- Right side bar lies here -->
                <%- include("../partials/dashboard/components/rightside.ejs") -%>
                    <!-- Right side bar : ends -->

                   	<!-- Main Content Wrapper -->
			<main class="main-content w-full px-[var(--margin-x)] pb-8">
                  <!-- BreadCrumbs lies here -->
                  <%- include("../partials/dashboard/components/breadcrumbs.ejs", {breadcrumbs_active : "Account"}) -%>
                  <!-- BreadCrumbs : ends -->

				<div class="grid grid-cols-12 gap-4 pt-3 sm:gap-5 lg:gap-6 " x-data="{activeTab:'tabAccount'}" >
					<div class="col-span-12 lg:col-span-4">
						<div class="card p-4 sm:p-5">
							<div class="flex items-center space-x-4">
								<div class="avatar h-14 w-14">
									<img class="rounded-full" src="/limestone/images/avatar/avatar-20.jpg" alt="avatar" />
								</div>
								<div>
                                    <h3 class="text-base font-medium text-slate-700 dark:text-navy-100">
                                        Travis Fuller
                                        <div class="inline-block space-x-2">
                                            <img x-tooltip="'Award Level 1'" class="h-6 w-6 inline-block" src="/limestone/images/awards/award-1.svg" alt="avatar">
                                            <img x-tooltip="'Award Level 2'" class="h-6 w-6 inline-block" src="/limestone/images/awards/award-6.svg" alt="avatar">
                                            <img x-tooltip="'Award Level 3'" class="h-6 w-6 inline-block" src="/limestone/images/awards/award-9.svg" alt="avatar">
                                        </div>
									</h3>
                                    <p class="text-xs+">
                                        Author
                                    </p>
                                    
								</div>
							</div>
							<ul class="mt-6 space-y-1.5 font-inter font-medium ">
								<li>
                                    <a 
                                    @click="activeTab = 'tabAccount'" :class="activeTab === 'tabAccount' ? 'bg-primary text-white' : 'group hover:bg-slate-100 hover:text-slate-800 focus:bg-slate-100 focus:text-slate-800 dark:hover:bg-navy-600 dark:hover:text-navy-100 dark:focus:bg-navy-600 dark:focus:text-navy-100 '"
                                    class="flex items-center space-x-2 rounded-lg px-4 py-2.5 tracking-wide  outline-none transition-all dark:bg-accent" href="#account">
										<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5" fill="none" viewBox="0 0 24 24" stroke="currentColor">
											<path stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5" d="M5.121 17.804A13.937 13.937 0 0112 16c2.5 0 4.847.655 6.879 1.804M15 10a3 3 0 11-6 0 3 3 0 016 0zm6 2a9 9 0 11-18 0 9 9 0 0118 0z" />
										</svg>
										<span>Account</span>
									</a>
								</li>
								<li>
                                    <a 
                                    @click="activeTab = 'tabSettings'" :class="activeTab === 'tabSettings' ? 'bg-primary text-white' : 'group hover:bg-slate-100 hover:text-slate-800 focus:bg-slate-100 focus:text-slate-800 dark:hover:bg-navy-600 dark:hover:text-navy-100 dark:focus:bg-navy-600 dark:focus:text-navy-100 '"
                                    class="flex space-x-2 rounded-lg px-4 py-2.5 tracking-wide outline-none transition-all" href="#settings">
                                    <svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-slate-400 transition-colors group-hover:text-slate-500 group-focus:text-slate-500 dark:text-navy-300 dark:group-hover:text-navy-200 dark:group-focus:text-navy-200" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5">
                                        <path stroke-linecap="round" stroke-linejoin="round" d="M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 002.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 001.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 00-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 00-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 00-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 00-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 001.066-2.573c-.94-1.543.826-3.31 2.37-2.37.996.608 2.296.07 2.572-1.065z"></path>
                                        <path stroke-linecap="round" stroke-linejoin="round" d="M15 12a3 3 0 11-6 0 3 3 0 016 0z"></path>
                                      </svg>

										<span>Settings</span>
									</a>
								</li>
								<li>
                                    <a 
                                    @click="activeTab = 'tabSecurity'" :class="activeTab === 'tabSecurity' ? 'bg-primary text-white' : 'group hover:bg-slate-100 hover:text-slate-800 focus:bg-slate-100 focus:text-slate-800 dark:hover:bg-navy-600 dark:hover:text-navy-100 dark:focus:bg-navy-600 dark:focus:text-navy-100 '"
                                    class="flex space-x-2 rounded-lg px-4 py-2.5 tracking-wide outline-none transition-all" href="#">
										<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-slate-400 transition-colors group-hover:text-slate-500 group-focus:text-slate-500 dark:text-navy-300 dark:group-hover:text-navy-200 dark:group-focus:text-navy-200" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5">
											<path stroke-linecap="round" stroke-linejoin="round" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z" />
										</svg>
										<span>Security</span>
									</a>
								</li>
								<li>
                                    <a 
                                    @click="activeTab = 'tabBilling'" :class="activeTab === 'tabBilling' ? 'bg-primary text-white' : 'group hover:bg-slate-100 hover:text-slate-800 focus:bg-slate-100 focus:text-slate-800 dark:hover:bg-navy-600 dark:hover:text-navy-100 dark:focus:bg-navy-600 dark:focus:text-navy-100 '"
                                    class="flex space-x-2 rounded-lg px-4 py-2.5 tracking-wide outline-none transition-all" href="#billings">
										<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-slate-400 transition-colors group-hover:text-slate-500 group-focus:text-slate-500 dark:text-navy-300 dark:group-hover:text-navy-200 dark:group-focus:text-navy-200" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5">
											<path stroke-linecap="round" stroke-linejoin="round" d="M20 7l-8-4-8 4m16 0l-8 4m8-4v10l-8 4m0-10L4 7m8 4v10M4 7v10l8 4" />
										</svg>
										<span>Cards & Billings</span>
									</a>
								</li>
								<li>
                                    <a 
                                    @click="activeTab = 'tabPrivacy'" :class="activeTab === 'tabPrivacy? 'bg-primary text-white' : 'group hover:bg-slate-100 hover:text-slate-800 focus:bg-slate-100 focus:text-slate-800 dark:hover:bg-navy-600 dark:hover:text-navy-100 dark:focus:bg-navy-600 dark:focus:text-navy-100 '"
                                    class=" flex space-x-2 rounded-lg px-4 py-2.5 tracking-wide outline-none transition-all hover:bg-slate-100 hover:text-slate-800 focus:bg-slate-100 focus:text-slate-800 dark:hover:bg-navy-600 dark:hover:text-navy-100 dark:focus:bg-navy-600 dark:focus:text-navy-100" href="#">
										<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-slate-400 transition-colors group-hover:text-slate-500 group-focus:text-slate-500 dark:text-navy-300 dark:group-hover:text-navy-200 dark:group-focus:text-navy-200" fill="none" viewBox="0 0 24 24" stroke="currentColor" stroke-width="1.5">
											<path stroke-linecap="round" stroke-linejoin="round" d="M19 11H5m14 0a2 2 0 012 2v6a2 2 0 01-2 2H5a2 2 0 01-2-2v-6a2 2 0 012-2m14 0V9a2 2 0 00-2-2M5 11V9a2 2 0 012-2m0 0V5a2 2 0 012-2h6a2 2 0 012 2v2M7 7h10" />
										</svg>
										<span> Privacy & data </span>
									</a>
								</li>
							</ul>
						</div>
					</div>
					<div class="col-span-12 lg:col-span-8 tab-content" >

                        <!-- account tab -->
                        <%- include("./account_tabs/account-overview.ejs") -%>
                        <!-- account-tab:end -->
						
                        <!-- Secondtab section -->
                        <%- include("./account_tabs/account-settings.ejs") -%>
                        <!-- secondTab:end -->

                          <!-- thirdtab section -->
                          <%- include("./account_tabs/account-security.ejs") -%>
                        <!-- thirdTab:end -->

                         <!-- fourtab section -->
                         <%- include("./account_tabs/account-billings.ejs") -%>
                         <!-- fourTab:end -->

                         <!-- fifthtab section -->
                         <%- include("./account_tabs/account-privacy.ejs") -%>
                         <!-- fifthTab:end -->

					</div>
				</div>
			</main>
        </div>
        <!-- 
        This is a place for Alpine.js Teleport feature 
        @see https://alpinejs.dev/directives/teleport
      -->
        <div id="x-teleport-target"></div>
        <script>
            window.addEventListener("DOMContentLoaded", () => Alpine.start());
        </script>
</body>

</html>