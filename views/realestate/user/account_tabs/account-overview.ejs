 <!-- first tab section -->
 
 <div x-show="activeTab === 'tabAccount'" x-transition:enter="transition-all duration-500 easy-in-out" x-transition:enter-start="opacity-0 [transform:translate3d(1rem,0,0)]" x-transition:enter-end="opacity-100 [transform:translate3d(0,0,0)]" >
    
       <!-- hear lies balance -->
       <%-include("../../partials/dashboard/components/balance.ejs", {Deposit_withdraw_button_show : false}) -%>
           <!-- ### -->
        
      <div class="card py-3 px-4 sm:px-5 my-4">
        <div class="space-y-4">
            <div class="space-y-3 text-xs+">
                <div class="flex justify-between">
                    <p class="font-medium text-slate-700 dark:text-navy-100">
                      Name
                    </p>
                    <p class="text-right"> <%= user.fname %> <%= user.lname %></p>
                  </div>
                  <div class="flex justify-between">
                    <p class="font-medium text-slate-700 dark:text-navy-100">
                      Email
                    </p>
                    <p class="text-right"> <%= user.email %></p>
                  </div>
                <div class="flex justify-between">
                <p class="font-medium text-slate-700 dark:text-navy-100">
                  D.O.B.
                </p>
                <p class="text-right"> <%= user.dob %></p>
              </div>
              <div class="flex justify-between">
                <p class="font-medium text-slate-700 dark:text-navy-100">
                  phone number
                </p>
                <p class="text-right"> <%= user.mobile %></p>
              </div>
              <div class="flex justify-between">
                <p class="font-medium text-slate-700 dark:text-navy-100">
                  Country 
                </p>
                <p class="text-right"> <%= user.country %> </p>
              </div>
              <div class="flex justify-between">
                <p class="font-medium text-slate-700 dark:text-navy-100">
                  Sex
                </p>
                <p class="text-right"> <%= user.gender %></p>
              </div>
              <div class="flex justify-between">
                <p class="font-medium text-slate-700 dark:text-navy-100">
                  Balance 
                </p>
                <p class="text-right"> $<%= user.balance %> </p>
              </div>
              <div class="flex justify-between">
                <p class="font-medium text-slate-700 dark:text-navy-100">
                  kyc verification 
                </p>
                <p class="text-right"> <%= user.kyc %> </p>
              </div>
            </div>
          </div>
      </div>
 
</div>
<!-- firstTab:end -->